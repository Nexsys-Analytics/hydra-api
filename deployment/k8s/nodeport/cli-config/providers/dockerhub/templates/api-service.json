{
  "apiVersion": "v1",
  "kind": "Service",
  "metadata": {
    "name": "api-svc-${CURRENT_WORKING_ENVIRONMENT}"
  },
  "spec": {
    "ports": [
      {
        "port": 80,
        "targetPort": "api-${CURRENT_WORKING_ENVIRONMENT}-srv",
        "protocol": "TCP",
        "name": "http",
        "nodePort": ${spec_ports_nodePort}
      }
    ],
    "type": "NodePort",
    "selector": {
      "app": "api-${CURRENT_WORKING_ENVIRONMENT}"
    }
  }
}
